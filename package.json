{
  "name": "joyeria-elegante",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:clean": "vite --force",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "test:payments": "node scripts/test-payments.js",
    "test:payments:watch": "nodemon scripts/test-payments.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:api": "vitest run tests/api",
    "test:api:watch": "vitest tests/api",
    "test:backend": "vitest run backend",
    "test:frontend": "vitest run --project frontend-unit",
    "test:performance": "node performance/artillery.yml && node performance/lighthouse.js",
    "test:mutation": "stryker run",
    "test:mutation:quick": "stryker run --concurrency 2 --mutate src/components/**/*.ts*",
    "test:mutation:critical": "stryker run --mutate src/services/**/*.ts --mutate src/utils/**/*.ts",
    "test:mutation:report": "stryker run && open reports/mutation/mutation-report.html",
    "test:contract": "vitest run tests/contract",
    "test:contract:watch": "vitest tests/contract",
    "test:contract:publish": "pact-broker publish pacts --consumer-app-version=$npm_package_version --broker-base-url=$PACT_BROKER_BASE_URL --broker-token=$PACT_BROKER_TOKEN",
    "test:all": "npm run test:backend && npm run test:frontend && npm run test:api && npm run test:e2e",
    "test:full": "npm run test:all && npm run test:mutation && npm run test:contract"
  },
  "dependencies": {
    "framer-motion": "^11.11.17",
    "lucide-react": "^0.460.0",
    "nodemailer": "^6.9.15",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-router-dom": "^6.28.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@faker-js/faker": "^10.1.0",
    "@pact-foundation/pact": "^16.0.2",
    "@playwright/test": "^1.56.1",
    "@stryker-mutator/core": "^9.3.0",
    "@stryker-mutator/typescript-checker": "^9.3.0",
    "@stryker-mutator/vitest-runner": "^9.3.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.1",
    "@types/nodemailer": "^7.0.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-plugin-postcss-modules": "^2.0.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "fast-check": "^4.3.0",
    "globals": "^16.5.0",
    "jsdom": "^27.1.0",
    "postcss": "^8.4.47",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.15.0",
    "vite": "^5.4.10",
    "vitest": "^2.1.8"
  }
}