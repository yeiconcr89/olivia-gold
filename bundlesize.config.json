{
  "files": [
    {
      "path": "dist/assets/index*.js",
      "maxSize": "250kb",
      "compression": "gzip"
    },
    {
      "path": "dist/assets/index*.css", 
      "maxSize": "50kb",
      "compression": "gzip"
    },
    {
      "path": "dist/assets/vendor*.js",
      "maxSize": "500kb",
      "compression": "gzip"
    }
  ],
  "ci": {
    "trackBranches": ["main", "develop"],
    "repoBranch": "main"
  },
  "defaultCompression": "gzip",
  "stripHash": true
}